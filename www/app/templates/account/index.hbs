


<div class="alert ">

 <div class="alert index_block2 table-responsive">

{{#if model.workers}}












  <table class="table table-striped table-condensed">
  <thead style="color:#000; background-color:rgba(0, 0, 0, 0.13);">
      <tr>
        <th>ID</th>
        <th>Хешрейт</th>
     
        <th>Шара</th>
      </tr>
    </thead>
    <tbody>
      {{#each-in model.workers as |k v|}}
        <tr class="{{if v.offline "warning" "success"}}">
          <td>{{k}}</td>
          <td>{{format-hashrate v.hr}}</td>
     
          <td>{{format-relative (seconds-to-ms v.lastBeat)}}</td>


	



</tr>
      {{/each-in}}
    </tbody>
  </table>
  {{else}}
    <h3>No workers online</h3>
  {{/if}}

</div>
</div>
















